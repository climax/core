{
  "extends": "tslint-config-airbnb",
  "linterOptions": {
    "exclude": [
      "./lib/**/types.ts"
    ]
  },
  "rules": {
    "import-name": [false],
    "quotemark": [true, "single"],
    "semicolon": [true, "never"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
